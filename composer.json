{
    "name": "nilsenj/toastr-laravel",
    "description": "toastr.js for Laravel 5.* easy notification like growl for example",
    "keywords": ["toastr", "notification", "laravel", "php"],
    "homepage": "https://github.com/nilsenj/toastr-laravel",
    "type":    "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamal Nasser",
            "email": "kamal@kamalnasser.net"
        }, 
        {
            "name": "Ivan Nikolenko",
            "email": "nikoleivan@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.0.0",
        "illuminate/session": ">=5.0.0"
    },

    "autoload": {
        "psr-4": {
            "nilsenj\\Toastr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nilsenj\\Toastr\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "master": "1.1.x-dev"
        }
    }
}
